root = true
# EditorConfig is awesome: https://EditorConfig.org

# Don't use tabs for indentation.
[*]
indent_style = space
charset = utf-8
end_of_line = lf

[*.{cs,csx}]
indent_size = 4

# XML files
[*.{csproj,proj,projitems,shproj,props,targets,config,nuspec,resx,vsixmanifest,vsct}]
indent_size = 2

# Script files
[*.{json,ps1,psd1,psm1,sh}]
indent_size = 2

# Dotnet code style settings:
[*.cs]

